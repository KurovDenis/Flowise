# Flowise Configuration
NODE_ENV=production
PORT=3000

# Database Configuration
DATABASE_PATH=/app/database
SECRETKEY_PATH=/app/secretkey

# Logging Configuration
LOG_LEVEL=info
LOG_PATH=/app/logs

# Security Configuration
FLOWISE_USERNAME=admin
FLOWISE_PASSWORD=admin123

# Evently API Configuration (for custom tools)
EVENTLY_API_URL=http://evently-api:5000
EVENTLY_API_TOKEN=your_jwt_token_here

# Optional: PostgreSQL Configuration
# DB_TYPE=postgres
# DB_HOST=postgres
# DB_PORT=5432
# DB_USERNAME=flowise
# DB_PASSWORD=flowise_password
# DB_DATABASE=flowise

# Optional: Redis Configuration
# REDIS_HOST=redis
# REDIS_PORT=6379
# REDIS_PASSWORD=

# Optional: External Database (if not using PostgreSQL)
# EXTERNAL_DB_HOST=your-db-host
# EXTERNAL_DB_PORT=5432
# EXTERNAL_DB_USERNAME=your-username
# EXTERNAL_DB_PASSWORD=your-password
# EXTERNAL_DB_DATABASE=flowise

# Optional: Email Configuration
# EMAIL_FROM=noreply@yourdomain.com
# EMAIL_SMTP_HOST=smtp.gmail.com
# EMAIL_SMTP_PORT=587
# EMAIL_SMTP_USER=your-email@gmail.com
# EMAIL_SMTP_PASS=your-app-password

# Optional: Authentication
# JWT_SECRET=your-jwt-secret-key
# JWT_EXPIRES_IN=7d

# Optional: Rate Limiting
# RATE_LIMIT_WINDOW_MS=900000
# RATE_LIMIT_MAX_REQUESTS=100

# Optional: CORS Configuration
# CORS_ORIGIN=http://localhost:3000
# CORS_CREDENTIALS=true

# Optional: File Upload
# MAX_FILE_SIZE=10485760
# UPLOAD_PATH=/app/uploads

# Optional: Webhook Configuration
# WEBHOOK_URL=https://your-webhook-url.com
# WEBHOOK_SECRET=your-webhook-secret

# Optional: Monitoring
# ENABLE_METRICS=true
# METRICS_PORT=9090

# Optional: Debug Mode
# DEBUG=false
# VERBOSE=false
